import environnement.interfaces.EnvInfos
import environnement.interfaces.EnvUpdate
import java.util.List
import trace.Action
import environnement.interfaces.CellInfo

namespace general{
	
	ecosystem Environnement {
		provides envInfos: EnvInfos
		provides envUpdate: EnvUpdate
		provides getCellInfo: CellInfo
		
		species Cell(actions : List[Action]) {
			provides cellInfos: CellInfo
		}
	}
	
}